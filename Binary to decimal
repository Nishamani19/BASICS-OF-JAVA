import java.util.*;
import java.util.Scanner;

class Main 
{
    public static void main(String[]args)
    {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int decimal=0;
        int p=1;
        while(a!=0)
         {
             int k=a%10;
             decimal = decimal + k * p;
             p = p*2;
             a/=10;
         }
         System.out.print(decimal);
    }
}
